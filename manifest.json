{
  "manifest_version": 3,
  "name": "我的起始頁",
  "version": "1.5.7",
  "description": "一個美觀的自訂新標籤頁和主頁，包含搜尋、書籤管理和天氣小工具",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "author": "AntoninaxMains",
  "homepage_url": "https://github.com/AntoninaxMains/home"
}

